<?xml version="1.0" encoding="utf-8"?>
<odoo noupdate="1">
  <record id="uom_categ_eco" model="uom.category">
    <field name="name">Ecoservice</field>
    <field name="uom_template" eval="True"/>
  </record>
  <record id="eco_uom_piece" model="uom.uom">
